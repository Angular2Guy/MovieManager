{
  "name": "moviemanager",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --hmr --proxy-config proxy.conf.json",
    "build": "ng build --localize --configuration production",
    "test": "ng test --browsers ChromeHeadless --watch=false",
    "test-chromium": "ng test --browsers ChromiumHeadless --watch=false",
    "test-local": "ng test --browsers Chromium --watch=true",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "postinstall": "ngcc",
    "prebuild": "rimraf ./dist && mkdirp ./dist",
    "postbuild": "npm run deploy",
    "predeploy": "rimraf ../../main/resources/static/*",
    "deploy": "cpx 'base/*' dist/testproject/ && cpx 'dist/testproject/**' ../../main/resources/static/"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^12.0.1",
    "@angular/common": "^12.0.1",
    "@angular/compiler": "^12.0.1",
    "@angular/core": "^12.0.1",
    "@angular/forms": "^12.0.1",
    "@angular/localize": "^12.0.1",
    "@angular/platform-browser": "^12.0.1",
    "@angular/platform-browser-dynamic": "^12.0.1",
    "@angular/router": "^12.0.1",
    "bootstrap": "^5.0.1",
    "core-js": "^2.5.7",
    "@popperjs/core": "^2.9.2",
    "rxjs": "^6.6.3",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.0.1",
    "@angular/cli": "^12.0.1",
    "@angular/compiler-cli": "^12.0.1",
    "@angular/language-service": "^12.0.1",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.5",
    "@types/node": "^12.11.1",
    "cpx": "^1.5.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-junit-reporter": "~1.2.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "mkdirp": "^0.5.1",
    "npm-run-all": "^4.1.5",
    "protractor": "~7.0.0",
    "rimraf": "^3.0.2",
    "ts-node": "~7.0.1",
    "typescript": "~4.2.4",
    "eslint": "^7.6.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "30.7.6",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "@angular-eslint/builder": "^12.0.0",
    "@angular-eslint/eslint-plugin": "^12.0.0",
    "@angular-eslint/eslint-plugin-template": "^12.0.0",
    "@angular-eslint/schematics": "^12.0.0",
    "@angular-eslint/template-parser": "^12.0.0",
    "@typescript-eslint/eslint-plugin": "4.16.1",
    "@typescript-eslint/parser": "4.16.1"
  }
}
