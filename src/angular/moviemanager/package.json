{
  "name": "moviemanager",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json",
    "build": "ng build --prod --aot --optimization=true --buildOptimizer=true --output-path dist",
    "test": "ng test --browsers ChromeHeadless --watch=false",
    "test-chromium": "ng test --browsers ChromiumHeadless --watch=false",
    "test-local": "ng test --browsers Chromium --watch=true",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "prebuild": "rimraf ./dist && mkdirp ./dist",
    "postbuild": "npm run deploy",
    "predeploy": "rimraf ../../main/resources/static && mkdirp ../../main/resources/static",
    "deploy": "copyfiles -f dist/** ../../main/resources/static/"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.14",
    "@angular/common": "^8.2.14",
    "@angular/compiler": "^8.2.14",
    "@angular/core": "^8.2.14",
    "@angular/forms": "^8.2.14",
    "@angular/platform-browser": "^8.2.14",
    "@angular/platform-browser-dynamic": "^8.2.14",
    "@angular/router": "^8.2.14",
    "bootstrap": "^4.3.1",
    "core-js": "^2.5.7",
    "jquery": "^3.4.0",
    "ngx-virtual-scroller": "^1.0.11",
    "popper.js": "^1.14.4",
    "rxjs": "^6.5.4",
    "zone.js": "^0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.25",
    "@angular/cli": "^8.3.25",
    "@angular/compiler-cli": "^8.2.14",
    "@angular/language-service": "^8.2.14",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.5",
    "@types/node": "~10.12.0",
    "codelyzer": "~5.0.1",
    "copyfiles": "^2.1.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.4",
    "karma-junit-reporter": "~1.2.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "mkdirp": "^0.5.1",
    "npm-run-all": "^4.1.5",
    "protractor": "~5.4.1",
    "rimraf": "^2.6.1",
    "ts-node": "~7.0.1",
    "tslint": "~5.15.0",
    "typescript": "~3.5.3"
  }
}
